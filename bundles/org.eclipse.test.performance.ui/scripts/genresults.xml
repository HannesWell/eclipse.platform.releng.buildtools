<project default="run">

	<target name="run">
		
		<java jar="${basedir}/../../../startup.jar" fork="true" maxmemory="256m">
			<arg line="-application org.eclipse.test.performance.ui.resultGenerator" />
			<arg line="${args}" />
			<env key="LD_LIBRARY_PATH" value="${basedir}/../../.." />
			<sysproperty key="eclipse.perf.dbloc" value="${dbloc}" />
		</java>
	</target>
</project>
